{
  "name": "JSJot",
  "version": "2.0.0",
  "main": "app.js",
  "description": "Collaborative JavaScript console logger.",
  "author": "Connor Wiebe",
  "license": "MIT",
  "scripts": {
    "start-dev": "concurrently --kill-others-on-fail \"yarn run server\" \"yarn run client\"",
    "server": "NODE_ENV=development nodemon -i client --trace-warnings app.js",
    "client": "cd client && yarn start"
  },
  "dependencies": {
    "bluebird": "^3.5.3",
    "compression": "^1.7.3",
    "connect-session-knex": "^2.1.0",
    "crypto-alphanumeric-id": "^2.0.0",
    "dotenv": "10.0.0",
    "express": "^4.17.3",
    "express-ip": "^1.0.3",
    "express-session": "^1.15.6",
    "express-ws": "^5.0.2",
    "haikunator": "^2.1.1",
    "helmet": "^4.6.0",
    "isbot-fast": "^1.0.0",
    "knex": "0.95.6",
    "pg": "^8.6.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "serve-favicon": "^2.5.0",
    "statuses": "^2.0.1"
  },
  "devDependencies": {
    "concurrently": "6.2.0"
  }
}
